@using Microsoft.AspNet.Identity
<!DOCTYPE html>
<html>
<head>

    <title>CoCycle Admin</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/Assets/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="~/Content/Assets/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/Content/Assets/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="~/Content/Assets/plugins/jqvmap/jqvmap.min.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="~/Content/Assets/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Content/Assets/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Content/Assets/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">
    <!-- Theme style -->
    <link href="~/Content/Assets/dist/css/adminlte.css" rel="stylesheet" />

    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/Content/Assets/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Content/Assets/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/Content/Assets/plugins/summernote/summernote-bs4.css">
    <link rel="stylesheet" href="~/Content/Assets/plugins/toastr/toastr.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini layout-fixed">

    <div class="wrapper">

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                @*<li class="nav-item d-none d-sm-inline-block">
                    <a href="index3.html" class="nav-link">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link">Contact</a>
                </li>*@
            </ul>

            <!-- SEARCH FORM -->
            <!--<form class="form-inline ml-3">
                <div class="input-group input-group-sm">
                    <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
                    <div class="input-group-append">
                        <button class="btn btn-navbar" type="submit">
                            <i class="fas fa-search"></i>
                        </button>
                    </div>
                </div>
            </form>-->

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Messages Dropdown Menu javascript:document.getElementById('logoutForm').submit()-->
                <li class="btn btn-light border-danger"><a href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>

            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="index3.html" class="brand-link">
                <img src="~/Content/Assets/dist/img/Cocycle-name-transparent._smallpng.png" alt="Cocycle Logo" class="brand-image "
                     style="opacity: .8">
                <span class="brand-text font-weight-light">&nbsp;</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">

                    </div>
                    <div class="info">
                        @if (Request.IsAuthenticated)
                        {
                            using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                            {
                                @Html.AntiForgeryToken()

                                <ul class="nav navbar-nav navbar-right">
                                    <li>
                                        @Html.ActionLink(User.Identity.GetUserName(),"")
                                    </li>

                                </ul>
                            }
                        }

                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <li class="nav-item has-treeview menu-open">
                            <a href="@Url.Action("Index","Home")" class="nav-link active">
                                <i class="fas fa-chart-line"></i>
                                <p>
                                    Dashboard
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fas fa-users"></i>
                                <p>
                                    Users
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">

                                <li class="nav-item">
                                    <a href="@Url.Action("ViewAll","ManageUser")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Manage users</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ChangeRole","ManageUser")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Manage Roles</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fas fa-route"></i>
                                <p>
                                    Routes
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("Index","Routes")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>All Routes</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ViewRequested","Routes")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Requested Routes</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fas fa-people-arrows"></i>
                                <p>
                                    Route Groups
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("ViewRequestbyRoute","RouteGroups")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>View Routewise</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("Index","RouteGroups")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>View All Members</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ViewRequest","RouteGroups")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Pending Request</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fas fa-biking"></i>
                                <p>
                                    Arranged Rides
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("Index","Arrangeds")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>View All</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ViewScheduled","Arrangeds")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p> Scheduled </p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ViewCompleted","Arrangeds")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p> Completed </p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("RequestRide","Arrangeds")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Create Request</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fas fa-tools"></i>
                                <p>
                                    Masters
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item ">
                                    <a href="@Url.Action("Index","States")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>States</p>
                                    </a>
                                    <a href="@Url.Action("Index","Areas")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Areas</p>
                                    </a>
                                    <a href="@Url.Action("Index","PostCodes")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>PostCodes</p>
                                    </a>
                                    <a href="@Url.Action("Index","EmailAccounts")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Email Config</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div>
                @RenderBody()
            </div>
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Designed and Developed By &copy;  <a href="http://I-diligence.com">I-diligence.com</a>.</strong>
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/Content/Assets/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/Assets/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>$.widget.bridge('uibutton', $.ui.button)</script>
    <!-- Bootstrap 4 -->
    <script src="~/Content/Assets/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>



    <!-- ChartJS -->
    <!-- JQVMap -->
    <script src="~/Content/Assets/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/Content/Assets/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <!-- daterangepicker -->
    <script src="~/Content/Assets/plugins/moment/moment.min.js"></script>
    <script src="~/Content/Assets/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/Content/Assets/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="~/Content/Assets/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/Content/Assets/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>

    <script src="~/Content/Assets/plugins/toastr/toastr.min.js"></script>
    <script src="~/Content/Assets/plugins/sweetalert2/sweetalert2.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/Assets/dist/js/adminlte.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/Content/Assets/dist/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/Assets/dist/js/demo.js"></script>


    @RenderSection("scripts", required: false)
    @RenderSection("Style", required: false)
</body>
</html>